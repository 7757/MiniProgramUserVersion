/*************弹框****************/

.weui-demo-dialog {
  visibility: hidden;
  opacity: 0;
  transition: all 0.3s;
  margin: 0px auto;
}

.weui-half-screen-dialog {
  transition: all 0.3s;
  transform: translateY(100%);
  padding-bottom: 40rpx;
}

.weui-demo-dialog_show {
  visibility: visible;
  opacity: 1;
}

.weui-demo-dialog_show .weui-half-screen-dialog {
  transform: translateY(0);
}

