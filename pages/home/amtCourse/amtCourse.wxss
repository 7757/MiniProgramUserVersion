/**************************自定义radio样式**************/

/* 单选框未选中 */

radio .wx-radio-input {
  position: absolute;
  width: 38rpx;
  height: 38rpx;
  border: 1rpx solid rgba(50, 133, 139, 1);
  right: 50rpx;
}

/* 选中后的 背景样式*/

radio .wx-radio-input.wx-radio-input-checked {
  border-color: #008489;
  background: #008489;
}

/* 单选框选中后对勾样式 */

radio .wx-radio-input.wx-radio-input-checked::before {
  text-align: center;
  font-size: 28rpx;
  color: #fff;
}

/**确定按钮**/

.btnqd {
  width: 100%;
  margin-top: 40rpx;
  height: 90rpx;
  background: rgba(0, 132, 137, 1);
  border-radius: 10rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 90rpx;
  text-align: center;
}

.head {
  visibility: hidden;
  opacity: 0;
  transition: all 0.3s;
}

.show {
  visibility: visible;
  opacity: 1;
  transition: all 0.3s;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/*radio未选中时样式 */

radio .wx-radio-input {
  width: 42rpx;
  height: 42rpx;
  border-radius: 8rpx;
  border: 2rpx solid rgba(0, 132, 137, 1);
}

/* 选中后的 背景样式  */

radio .wx-radio-input.wx-radio-input-checked {
  background: transparent;
}

/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */

radio .wx-radio-input.wx-radio-input-checked::before {
  color: rgba(0, 132, 137, 1);
  line-height: 40rpx;
  text-align: center;
  background: transparent;
  width: 42rpx;
  height: 42rpx;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}







