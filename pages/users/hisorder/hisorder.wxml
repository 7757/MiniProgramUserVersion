<view class="container">

  <view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" data-index="1" wx:for="{{items}}" wx:key="" data-index="{{index}}">
    <view class="content">
      <view class="h_content">
        <!-- 小标题 -->
        <view class="h_title">{{item.f_title}}</view>
        <!-- 简介 -->
        <view class="h_summary">订单时间：{{item.f_time}}</view>
        <viwe class="h_button" bindtap="infos" data-status="{{item.transferStatus}}">{{item.f_botten}}
          <image  wx:if="{{item.transferStatus}}" src="https://sappbkt.oss-cn-shanghai.aliyuncs.com/home/e9944621aabe3ae3091e7278055580b.png"></image>
        </viwe>
      </view>
      <!--主图-->
      <view class="images">
        <image class="h_images" src="{{item.images}}"></image>
      </view>
    </view>
  </view>
</view>