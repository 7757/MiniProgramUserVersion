<view class="container">
  <view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" data-index="1" bindtouchstart="touchstart" bindtouchmove="touchmove" wx:for="{{items}}" wx:key="" data-index="{{index}}" >
    <view class="content" catchtap="navCourseHander">
      <!--主图-->
      <view class="images">
        <image class="f_images" src="{{item.images}}"></image>
      </view>
      <view class="f_content">
        <!-- 小标题 -->
        <view class="f_title">{{item.f_title}}</view>
        <!-- 简介 -->
        <view class="f_summary">{{item.f_summary}}</view>
        <!-- 价格 -->
        <view class="f_price">￥{{item.f_price}}</view>
        <view class="f_xap">
          <!-- 五角星和评分分数 -->
          <image class='f_xingp' src='{{item.f_xingp}}' />
          <view class="f_point">{{item.f_point}}</view>
        </view>
      </view>
    </view>
    <view class="del" catchtap="del" data-index="{{index}}">删除</view>
  </view>
</view> 